<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:orientation = "horizontal"
    tools:context="com.example.byoungjune.term.Main2Activity">


    <fragment
        android:id = "@+id/googleMap2"
        android:layout_width="400dp"
        class = "com.google.android.gms.maps.MapFragment"
        android:layout_below="@+id/btn_next2"
        android:layout_height="250dp" />

    <Button
        android:text="입력"
        android:layout_height="35dp"
        android:id="@+id/btn_next2"
        android:layout_width="128dp"
        android:background="@drawable/white"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="목표"
        android:layout_width="128dp"
        android:layout_height="35dp"
        android:layout_above="@+id/googleMap2"
        android:layout_toEndOf="@+id/button2"
        android:id="@+id/button5"
        android:background="@drawable/white" />

    <Button
        android:text="분석"
        android:layout_width="128dp"
        android:layout_height="35dp"
        android:id="@+id/button2"
        android:background="@drawable/white_sel"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/btn_next2" />

    <ListView
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:id="@+id/list"
        android:scrollbars="horizontal"
        android:headerDividersEnabled="false"
        android:background="@drawable/white"
        android:layout_marginTop="64dp"
        android:layout_below="@+id/googleMap2"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="TextView"
        android:layout_width="384dp"
        android:layout_marginTop="16dp"
        android:id="@+id/textView5"
        android:layout_height="30dp"
        android:layout_below="@+id/googleMap2"
        android:layout_alignParentStart="true"
        android:gravity="center_horizontal"
        android:textColor="#3498db"/>


</RelativeLayout>
